<?xml version="1.0" encoding="UTF-8"?>
<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Permissions administrateur pour Quebec Property Search</description>
    <hasActivationRequired>false</hasActivationRequired>
    <label>Quebec Property Admin</label>
    
    <applicationVisibilities>
        <application>Quebec_Property_Search_App</application>
        <visible>true</visible>
    </applicationVisibilities>
    
    <objectPermissions>
        <allowCreate>true</allowCreate>
        <allowDelete>true</allowDelete>
        <allowEdit>true</allowEdit>
        <allowRead>true</allowRead>
        <modifyAllRecords>true</modifyAllRecords>
        <object>Quebec_Property_Usage__c</object>
        <viewAllRecords>true</viewAllRecords>
    </objectPermissions>
    
    <classAccesses>
        <apexClass>QuebecPropertyController</apexClass>
        <enabled>true</enabled>
    </classAccesses>
    
    <classAccesses>
        <apexClass>QuebecPropertyAPIService</apexClass>
        <enabled>true</enabled>
    </classAccesses>
    
    <classAccesses>
        <apexClass>QuebecPropertyException</apexClass>
        <enabled>true</enabled>
    </classAccesses>
    
    <tabSettings>
        <tab>Quebec_Property_Search</tab>
        <visibility>Visible</visibility>
    </tabSettings>
    
    <tabSettings>
        <tab>Quebec_Property_Admin</tab>
        <visibility>Visible</visibility>
    </tabSettings>
</PermissionSet>
